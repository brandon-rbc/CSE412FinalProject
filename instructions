make sure you have postgresql installed
create new pycharm project
setup virtual environment
install pyside6: pip install PyCharm6
install psycopg2: pip install psycopg2-binary
create database: cmd line
    export PGPORT=8888
    export PGHOST=/tmp
    initdb /path/to/cse-412-class-project/database/
    pg_ctl -D /path/to/database/root/folder -o '-k /tmp' start
    createdb $USER
    change psql login info in backend/psql_handlers.py to correct info
    https://github.com/momenabdelkarim/cse-412-class-project/#application-database-sync
create tables: psql -d $USER -f create_tables.sql
insert tables: psql -d $USER -f fill_tables.sql

